{
  "name": "xterm-node-pty-websocket-example",
  "version": "1.0.0",
  "description": "A simple example of xterm.js and node-pty with websocket",
  "scripts": {
    "dev:client": "pnpm --prefix client dev",
    "dev:server": "pnpm --prefix server dev",
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\""
